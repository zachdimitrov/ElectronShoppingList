{
    "name": "shoppinglist",
    "version": "1.0.0",
    "description": "Electron example app",
    "main": "main.js",
    "scripts": {
        "start": "./node_modules/.bin/electron .",
        "test": "test",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/favicon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/favicon.png --prune=true --out=release-builds"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zachdimitrov/ElectronShoppingList"
    },
    "keywords": [
        "electron",
        "desktop",
        "shopping"
    ],
    "author": "Zach Dimitrov",
    "license": "MIT",
    "devDependencies": {
        "electron": "^1.8.2",
        "electron-packager": "^11.0.1"
    }
}